class Item {
  constructor(name, sellIn, quality) {
    this.hasToBeSoldInLessThan = days => {
      return this.sellIn.isLessThan(days);
    };

    this.increaseQuality = () => {
      this.quality = this.quality.increase();
    };

    this.update = () => {};

    this.name = name;
    this.sellIn = sellIn;
